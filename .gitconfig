[user]
	name = MISTER-H18
[init]
	defaultBranch = main
[core]
	editor = vim --nofork
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true    # use n and N to move between diff sections
	dark = true
	side-by-side = true
	line-numbers = true
[merge]
	conflictstyle = zdiff3
